function extractUniqueCharacters(str) {
    // Initialize an empty set to store unique characters
    let uniqueChars = new Set();

    // Iterate through each character in the string
    for (let char of str) {
        uniqueChars.add(char); // Add character to set (sets automatically handle duplicates)
    }

    // Convert set to string by joining its elements
    let result = [...uniqueChars].join('');

    return result;
}

// Example usage:
const inputString = "thequickbrownfoxjumpsoverthelazydog";
const uniqueCharacters = extractUniqueCharacters(inputString);

console.log("Input string:", inputString);
console.log("Unique characters:", uniqueCharacters);
